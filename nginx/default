server {
        listen 80 default_server;
        listen [::]:80 default_server;
        #listen 192.168.178.59:80 default_server;
        root /var/www/html;
        index index.php;

        server_name nextcloud.philleconnect;

        location / {
                try_files $uri $uri/ =404;
        }
        location ~ \.php(?:$|/) {
                include snippets/fastcgi-php.conf;
                fastcgi_pass nextcloud_fpm:9000;
        }
}
